{
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  },
  "opts": {
    "destination": "docs/",
    "encoding": "utf8",
    "readme": "./README.md",
    "theme_opts": {
      "default_theme": "dark",
      "homepageTitle": "Test Data Generator"
    },
    "recurse": true,
    "template": "node_modules/clean-jsdoc-theme",
    "verbose": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "index.mjs",
      "src",
      "README.md"
    ],
    "includePattern": ".+\\.mjs$",
    "excludePattern": ".+\\.json$"
  }
}
